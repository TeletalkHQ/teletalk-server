{
  "name": "teletalk-server",
  "author": "S-STALWART-S",
  "description": "teletalk messenger app (server)",
  "license": "GPT-3",
  "version": "1.0.0",
  "main": "index.ts",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/S-STALWART-S/teletalk-server"
  },
  "scripts": {
    "app-runner": "node --trace-warnings --no-warnings --loader ./loader.js ./index.ts",
    "build": "node ./esbuild/index.js",
    "check:format": "prettier --write  --list-different ./",
    "check:lint": "eslint ./",
    "check:lint:default": "eslint 'src/**/*.ts'",
    "check:security": "snyk test & snyk code test",
    "check:style": "npm run check:format && npm run check:lint && npm run check:types",
    "check:types": "tsc --noEmit",
    "dev": "npm run start:dev",
    "setup:alias:load": "./scripts/alias/loader.sh",
    "setup:alias:unload": "./scripts/alias/unloader.sh",
    "setup:git": "husky install",
    "start": "npm run start:production:local",
    "start:dev": "cross-env NODE_ENV=development nodemon ./index.ts",
    "start:production": "cross-env NODE_ENV=production npm run app-runner",
    "start:production:local": "cross-env NODE_ENV=production_local npm run app-runner",
    "test-runner": "ts-mocha ./index.ts",
    "test:coverage": "cross-env COVERAGE=true c8 npm run test:dev",
    "test:dev": "cross-env NODE_ENV=test_development npm run test-runner",
    "test:dev:generate-badge": "cross-env MOCHA_BADGE_GEN_OUTPUT=./testSrc/passedTests.svg npm run test:dev -- -- --reporter mocha-badge-generator",
    "test:production": "cross-env NODE_ENV=test_production npm run test-runner",
    "test:production:local": "cross-env NODE_ENV=test_production_local npm run test-runner"
  },
  "dependencies": {
    "@redis/json": "^1.0.4",
    "@sindresorhus/is": "^5.3.0",
    "@socket.io/cluster-adapter": "^0.2.2",
    "@socket.io/sticky": "^1.0.3",
    "@types/mocha": "^10.0.1",
    "axios": "^1.3.4",
    "check-fields": "^1.2.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "custom-typeof": "^3.0.4",
    "debug": "^4.3.4",
    "defer-to-connect": "^2.0.1",
    "dotenv": "^16.0.3",
    "fastest-validator": "^1.16.0",
    "fs-extra": "^11.1.0",
    "generate-password": "^1.7.0",
    "helmet": "^6.1.5",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "logger-chalker": "^4.0.5",
    "mongoose": "^7.0.4",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.2",
    "nodemailer": "^6.9.1",
    "nodemailer-smtp-transport": "^2.7.4",
    "pretty-error": "^4.0.0",
    "redis": "^4.6.5",
    "serve-favicon": "^2.5.0",
    "simple-trier": "^3.1.3",
    "socket.io": "^4.6.1",
    "utility-store": "^12.1.7",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@babel/eslint-parser": "^7.19.1",
    "@babel/preset-env": "^7.21.4",
    "@babel/register": "^7.21.0",
    "@cspell/eslint-plugin": "^6.29.2",
    "@faker-js/faker": "^7.6.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@stryker-mutator/core": "^6.4.2",
    "@stryker-mutator/mocha-runner": "^6.4.2",
    "@stryker-mutator/typescript-checker": "^6.4.2",
    "@types/chai": "^4.3.4",
    "@types/confusing-browser-globals": "^1.0.0",
    "@types/fs-extra": "^11.0.1",
    "@types/generate-password": "^1.5.1",
    "@types/jest": "^29.5.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/mongoose-unique-validator": "^1.0.7",
    "@types/node": "^18.15.3",
    "@types/socket.io": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "c8": "^7.13.0",
    "chai": "^4.3.7",
    "confusing-browser-globals": "^1.0.11",
    "esbuild": "^0.17.11",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^1.7.1",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-testing-library": "^5.10.2",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "lint-staged": "^13.2.0",
    "madge": "^6.0.0",
    "mocha": "^10.2.0",
    "mocha-badge-generator": "^0.10.6",
    "nodemon": "^2.0.21",
    "ping": "^0.4.4",
    "prettier": "2.8.4",
    "snyk": "^1.1118.0",
    "socket.io-client": "^4.6.1",
    "ts-jest": "^29.0.5",
    "ts-mocha": "^10.0.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tslib": "^2.5.0",
    "typescript": "^4.9.5",
    "yargs": "^17.7.1"
  },
  "engines": {
    "node": ">=16.10.0"
  },
  "keywords": [
    "teletalk",
    "messenger"
  ]
}
